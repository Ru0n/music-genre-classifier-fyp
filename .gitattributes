# Track large model files with Git LFS
model/*.keras filter=lfs diff=lfs merge=lfs -text
model/*.h5 filter=lfs diff=lfs merge=lfs -text
model/*.joblib filter=lfs diff=lfs merge=lfs -text

# Track audio files with Git LFS
backend/uploads/*.wav filter=lfs diff=lfs merge=lfs -text
backend/uploads/*.mp3 filter=lfs diff=lfs merge=lfs -text

# Track spectrogram images with Git LFS
backend/uploads/spectrograms/*.png filter=lfs diff=lfs merge=lfs -text